<view class='weui-cells'>
 <view class='weui-cell'    wx:for="{{getData}}" wx:key="company_phone" bindtap='telNum' data-phone='{{item.company_phone}}'>
 
 <view class='weui-cell__hd'> <image src="data:image/png;base64,{{item.company_pic}}" style='width:30px;height:30px;margin-right:5px;' mode='aspectFill'></image></view>
 <view class='weui-cell__bd'>{{item.company_name}}</view> 
<view class='weui-cell__ft weui-cell__ft_in-access' >{{item.company_phone}}</view>
 
 </view>
</view>